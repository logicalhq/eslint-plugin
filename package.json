{
  "name": "@logicalhq/eslint-plugin",
  "version": "1.0.0",
  "author": "Logical <foss@logical.work>",
  "license": "BSD-2-Clause",
  "description": "An opinioned ESLint plugin.",
  "main": "dist/index.js",
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "THANKS.md",
    "LICENSE.md",
    "CHANGELOG.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logicalhq/eslint-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/logicalhq/eslint-plugin/issues"
  },
  "homepage": "https://github.com/logicalhq/eslint-plugin#readme",
  "keywords": [
    "eslint-plugin"
  ],
  "scripts": {
    "build": "rollup -c",
    "fmt": "prettier --write \"./**/*.{ts,js,json,md}\"",
    "fmt:check": "prettier --list-different \"./**/*.{ts,js,json,md}\"",
    "lint": "DEBUG=eslint:cli-engine eslint --ext=js,ts .",
    "lint:fix": "yarn lint --fix",
    "lint:spelling": "cspell --config=.cspell.json \"**/*.{md,js,ts}\"",
    "precommit": "yarn lint-staged",
    "prepush": "yarn fmt:check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config ./test/jest.config.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.20.0"
  },
  "devDependencies": {
    "@logicalhq/commitlint-config": "^1.3.1",
    "@logicalhq/eslint-config-base": "^1.1.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@swc/core": "^1.2.171",
    "@swc/jest": "^0.2.20",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.27",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "commitlint": "^16.2.3",
    "cspell": "^5.19.7",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^28.0.0",
    "jest-junit": "^13.2.0",
    "lint-staged": "^12.4.0",
    "prettier": "^2.6.2",
    "rollup": "^2.70.2",
    "rollup-plugin-polyfill-node": "^0.9.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-essentials": "^9.1.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0"
  },
  "engines": {
    "node": ">=17"
  },
  "volta": {
    "node": "18.0.0",
    "yarn": "1.22.18"
  }
}
