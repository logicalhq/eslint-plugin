{
  "name": "@logicalhq/eslint-plugin",
  "version": "1.0.0",
  "author": "Logical <foss@logical.work>",
  "license": "BSD-2-Clause",
  "description": "An opinioned ESLint plugin.",
  "main": "dist/index.js",
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "THANKS.md",
    "LICENSE.md",
    "CHANGELOG.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logicalhq/eslint-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/logicalhq/eslint-plugin/issues"
  },
  "homepage": "https://github.com/logicalhq/eslint-plugin#readme",
  "keywords": [
    "eslint-plugin"
  ],
  "scripts": {
    "build": "rollup -c",
    "fmt": "prettier --write \"./**/*.{ts,js,json,md}\"",
    "fmt:check": "prettier --list-different \"./**/*.{ts,js,json,md}\"",
    "lint": "DEBUG=eslint:cli-engine eslint --ext=js,ts .",
    "lint:fix": "yarn lint --fix",
    "lint:spelling": "cspell --config=.cspell.json \"**/*.{md,js,ts}\"",
    "precommit": "yarn lint-staged",
    "prepush": "yarn fmt:check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config ./test/jest.config.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.30.4"
  },
  "devDependencies": {
    "@logicalhq/commitlint-config": "^1.3.1",
    "@logicalhq/eslint-config-base": "^1.1.0",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@swc/core": "^1.2.208",
    "@swc/jest": "^0.2.21",
    "@types/eslint": "^8.4.5",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "commitlint": "^17.0.3",
    "cspell": "^6.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-junit": "^14.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "rollup-plugin-polyfill-node": "^0.9.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-essentials": "^9.1.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0"
  },
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "18.0.0",
    "yarn": "1.22.18"
  }
}
